//AIMLAYER SEQUENCES

$sequence "aim_crouch_pistol_still_up" {
	"aim_crouch_pistol_still_up_up_right" "aim_crouch_pistol_still_up_up_center"  "aim_crouch_pistol_still_up_up_left"
	"aim_crouch_pistol_still_up_mid_right" "aim_crouch_pistol_still_up_mid_center"  "aim_crouch_pistol_still_up_mid_left"
	"aim_crouch_pistol_still_up_down_right" "aim_crouch_pistol_still_up_down_center" "aim_crouch_pistol_still_up_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

$sequence "aim_crouch_pistol_still_down" {
	"aim_crouch_pistol_still_down_up_right" "aim_crouch_pistol_still_down_up_center"  "aim_crouch_pistol_still_down_up_left"
	"aim_crouch_pistol_still_down_mid_right" "aim_crouch_pistol_still_down_mid_center"  "aim_crouch_pistol_still_down_mid_left"
	"aim_crouch_pistol_still_down_down_right" "aim_crouch_pistol_still_down_down_center" "aim_crouch_pistol_still_down_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

$sequence "aim_crouch_pistol_move_up" {
	"aim_crouch_pistol_move_up_up_right" "aim_crouch_pistol_move_up_up_center"  "aim_crouch_pistol_move_up_up_left"
	"aim_crouch_pistol_move_up_mid_right" "aim_crouch_pistol_move_up_mid_center"  "aim_crouch_pistol_move_up_mid_left"
	"aim_crouch_pistol_move_up_down_right" "aim_crouch_pistol_move_up_down_center" "aim_crouch_pistol_move_up_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

$sequence "aim_crouch_pistol_move_down" {
	"aim_crouch_pistol_move_down_up_right" "aim_crouch_pistol_move_down_up_center"  "aim_crouch_pistol_move_down_up_left"
	"aim_crouch_pistol_move_down_mid_right" "aim_crouch_pistol_move_down_mid_center"  "aim_crouch_pistol_move_down_mid_left"
	"aim_crouch_pistol_move_down_down_right" "aim_crouch_pistol_move_down_down_center" "aim_crouch_pistol_move_down_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}


//IDLE SEQUENCES

$sequence "crouch_pistol_idle_up" {
	"spartan_animations\crouch_pistol_idle.smd"
	activity "ACT_HL2MP_IDLE_CROUCH_PISTOL" 1
	addlayer	"aim_crouch_pistol_still_up"
	fadein 0.2
	fadeout 0.2
	fps 30
	loop
}

$sequence "crouch_pistol_idle_down" {
	"spartan_animations\crouch_pistol_idle.smd"
	activity "ACT_HL2MP_IDLE_CROUCH_PISTOL" 1
	addlayer	"aim_crouch_pistol_still_down"
	fadein 0.2
	fadeout 0.2
	fps 30
	loop
}

//MOVEMENT SEQUENCES

$sequence "move_crouch_pistol_up" {
	"crouch_pistol_move_S"
	"crouch_pistol_move_W"
	"crouch_pistol_move_N"
	"crouch_pistol_move_E"
	"crouch_pistol_move_S"
	activity "ACT_HL2MP_WALK_CROUCH_PISTOL" 1
	blend "move_yaw" -180 180
	addlayer	"aim_crouch_pistol_move_up"
	blendwidth 5
	fadein 0.2
	fadeout 0.2
	node "move_pistol"
	fps 30
	loop
	{ event AE_CL_PLAYSOUND 2 "HALO3_spartan.Step" }
	{ event AE_CL_PLAYSOUND 8 "HALO3_spartan.Step" }
	walkframe 145 LX LY
}

$sequence "move_crouch_pistol_down" {
	"crouch_pistol_move_S"
	"crouch_pistol_move_W"
	"crouch_pistol_move_N"
	"crouch_pistol_move_E"
	"crouch_pistol_move_S"
	activity "ACT_HL2MP_WALK_CROUCH_PISTOL" 1
	blend "move_yaw" -180 180
	addlayer	"aim_crouch_pistol_move_down"
	blendwidth 5
	fadein 0.2
	fadeout 0.2
	node "move_pistol"
	fps 30
	loop
	{ event AE_CL_PLAYSOUND 2 "HALO3_spartan.Step" }
	{ event AE_CL_PLAYSOUND 8 "HALO3_spartan.Step" }
	walkframe 145 LX LY
}

