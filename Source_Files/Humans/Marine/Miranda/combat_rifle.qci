//MOVEMENT SEQUENCES

$sequence "move_rifle_up" {
	"rifle_move_S"
	"rifle_move_W"
	"rifle_move_N"
	"rifle_move_E"
	"rifle_move_S"
	activity "ACT_RUN_AIM_PISTOL" 1
	blend "move_yaw" -180 180
	addlayer	"aim_rifle_move_up"
	blendwidth 5
	fadein 0.2
	fadeout 0.2
	node "move_missile"
	fps 30
