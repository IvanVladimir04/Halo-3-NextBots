//AIMLAYER SEQUENCES

$sequence "aim_crouch_missile_still_up" {
	"aim_crouch_missile_still_up_up_right" "aim_crouch_missile_still_up_up_center"  "aim_crouch_missile_still_up_up_left"
	"aim_crouch_missile_still_up_mid_right" "aim_crouch_missile_still_up_mid_center"  "aim_crouch_missile_still_up_mid_left"
	"aim_crouch_missile_still_up_down_right" "aim_crouch_missile_still_up_down_center" "aim_crouch_missile_still_up_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

$sequence "aim_crouch_missile_move_up" {
	"aim_crouch_missile_move_up_up_right" "aim_crouch_missile_move_up_up_center"  "aim_crouch_missile_move_up_up_left"
	"aim_crouch_missile_move_up_mid_right" "aim_crouch_missile_move_up_mid_center"  "aim_crouch_missile_move_up_mid_left"
	"aim_crouch_missile_move_up_down_right" "aim_crouch_missile_move_up_down_center" "aim_crouch_missile_move_up_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}


//IDLE SEQUENCES


$sequence "crouch_missile_idle_up" {
	"marine_animations\crouch_missile_idle.smd"
	activity "ACT_IDLE_RPG" 1
	addlayer	"aim_crouch_missile_still_up"
	fadein 0.2
	fadeout 0.2
	fps 30
	loop
}


//MOVEMENT SEQUENCES

$sequence "move_crouch_missile_up" {
	"crouch_missile_move_S"
	"crouch_missile_move_W"
	"crouch_missile_move_N"
	"crouch_missile_move_E"
	"crouch_missile_move_S"
	activity "ACT_RUN_RPG" 1
	blend "move_yaw" -180 180
	addlayer	"aim_crouch_missile_move_up"
	blendwidth 5
	fadein 0.2
	fadeout 0.2
	node "move_missile"
	fps 30
	loop
	{ event AE_CL_PLAYSOUND 2 "HALO3_human.Step" }
	{ event AE_CL_PLAYSOUND 8 "HALO3_human.Step" }
	walkframe 145 LX LY
}

$sequence "walk_crouch_missile_up" {
	"crouch_missile_walk_S"
	"crouch_missile_walk_W"
	"crouch_missile_walk_N"
	"crouch_missile_walk_E"
	"crouch_missile_walk_S"
	activity "ACT_WALK_RPG" 1
	blend "move_yaw" -180 180
	addlayer	"aim_crouch_missile_move_up"
	blendwidth 5
	fadein 0.2
	fadeout 0.2
	node "walk_pistol"
	fps 30
	loop
	{ event AE_CL_PLAYSOUND 2 "HALO3_human.Step" }
	{ event AE_CL_PLAYSOUND 8 "HALO3_human.Step" }
	walkframe 145 LX LY
}

