//IDLE SEQUENCES

$sequence "crouch_rifle_idle_down" {
	"marine_animations\crouch_rifle_idle.smd"
	activity "ACT_IDLE_RPG_RELAXED" 1
	addlayer	"aim_crouch_rifle_still_down"
	fadein 0.2
	fadeout 0.2
	fps 30
	loop
}

$sequence "crouch_rifle_idle_up" {
	"marine_animations\crouch_rifle_idle.smd"
