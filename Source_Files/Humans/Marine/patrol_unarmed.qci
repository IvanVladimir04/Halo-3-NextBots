//AIMLAYER SEQUENCES

$sequence "aim_patrol_unarmed_still_up" {
	"aim_patrol_unarmed_still_up_up_right" "aim_patrol_unarmed_still_up_up_center"  "aim_patrol_unarmed_still_up_up_left"
	"aim_patrol_unarmed_still_up_mid_right" "aim_patrol_unarmed_still_up_mid_center"  "aim_patrol_unarmed_still_up_mid_left"
	"aim_patrol_unarmed_still_up_down_right" "aim_patrol_unarmed_still_up_down_center" "aim_patrol_unarmed_still_up_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

$sequence "aim_patrol_unarmed_still_down" {
	"aim_patrol_unarmed_still_down_up_right" "aim_patrol_unarmed_still_down_up_center"  "aim_patrol_unarmed_still_down_up_left"
	"aim_patrol_unarmed_still_down_mid_right" "aim_patrol_unarmed_still_down_mid_center"  "aim_patrol_unarmed_still_down_mid_left"
	"aim_patrol_unarmed_still_down_down_right" "aim_patrol_unarmed_still_down_down_center" "aim_patrol_unarmed_still_down_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

$sequence "aim_patrol_unarmed_move_up" {
	"aim_patrol_unarmed_move_up_up_right" "aim_patrol_unarmed_move_up_up_center"  "aim_patrol_unarmed_move_up_up_left"
	"aim_patrol_unarmed_move_up_mid_right" "aim_patrol_unarmed_move_up_mid_center"  "aim_patrol_unarmed_move_up_mid_left"
	"aim_patrol_unarmed_move_up_down_right" "aim_patrol_unarmed_move_up_down_center" "aim_patrol_unarmed_move_up_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

$sequence "aim_patrol_unarmed_move_down" {
	"aim_patrol_unarmed_move_down_up_right" "aim_patrol_unarmed_move_down_up_center"  "aim_patrol_unarmed_move_down_up_left"
	"aim_patrol_unarmed_move_down_mid_right" "aim_patrol_unarmed_move_down_mid_center"  "aim_patrol_unarmed_move_down_mid_left"
	"aim_patrol_unarmed_move_down_down_right" "aim_patrol_unarmed_move_down_down_center" "aim_patrol_unarmed_move_down_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

//IDLE SEQUENCES

$sequence "patrol_unarmed_idle_down" {
	"marine_animations\patrol_unarmed_any_idle.smd"
	activity "ACT_IDLE_RELAXED" 1
	addlayer	"aim_patrol_unarmed_still_down"
	fadein 0.2
	fadeout 0.2
	fps 30
	loop
}

$sequence "patrol_unarmed_idle_up" {
	"marine_animations\patrol_unarmed_any_idle.smd"
	activity "ACT_IDLE_STIMULATED" 1
	addlayer	"aim_patrol_unarmed_still_up"
	fadein 0.2
	fadeout 0.2
	fps 30
	loop
}

$sequence "patrol_unarmed_idle_posing_1" {
	"marine_animations\patrol_unarmed_posing_var1.smd"
