//MOVEMENT SEQUENCES

$sequence "move_patrol_unarmed_up" {
	"patrol_unarmed_move_N"
	activity "ACT_RUN_STIMULATED" 1
	addlayer	"aim_patrol_unarmed_move_up"
	fadein 0.2
	fadeout 0.2
	node "move_missile"
	fps 30
	loop
	{ event AE_CL_PLAYSOUND 2 "HALO3_human.Step" }
	{ event AE_CL_PLAYSOUND 8 "HALO3_human.Step" }
	walkframe 145 LX LY
}

$sequence "move_patrol_unarmed_down" {
	"patrol_unarmed_move_N"
	activity "ACT_RUN_RELAXED" 1
	addlayer	"aim_patrol_unarmed_move_down"
