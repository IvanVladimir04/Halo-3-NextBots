//MOVEMENT SEQUENCES

$sequence "move_combat_missile_up" {
	"combat_missile_move_S"
	"combat_missile_move_W"
	"combat_missile_move_N"
	"combat_missile_move_E"
	"combat_missile_move_S"
	activity "ACT_RUN_RPG" 1
	blend "move_yaw" -180 180
	addlayer	"aim_combat_missile_move_up"
	blendwidth 5
	fadein 0.2
	fadeout 0.2
	node "move_missile"
	fps 30
	loop
	{ event AE_CL_PLAYSOUND 2 "HALO3_human.Step" }
