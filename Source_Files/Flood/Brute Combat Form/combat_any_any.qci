//COMBAT OVERLAY

$sequence "Combat_Fidget_Layer" {
	"combat_overlay"
	fadein 0.2
	fadeout 0.2
	fps 30
	loop
	delta
	hidden
}

//AIMLAYER SEQUENCES

$sequence "aim_combat_any_still_up" {
	"aim_combat_any_still_up_up_right" "aim_combat_any_still_up_up_center"  "aim_combat_any_still_up_up_left"
	"aim_combat_any_still_up_mid_right" "aim_combat_any_still_up_mid_center"  "aim_combat_any_still_up_mid_left"
	"aim_combat_any_still_up_down_right" "aim_combat_any_still_up_down_center" "aim_combat_any_still_up_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

$sequence "aim_combat_any_move_up" {
	"aim_combat_any_move_up_up_right" "aim_combat_any_move_up_up_center"  "aim_combat_any_move_up_up_left"
	"aim_combat_any_move_up_mid_right" "aim_combat_any_move_up_mid_center"  "aim_combat_any_move_up_mid_left"
	"aim_combat_any_move_up_down_right" "aim_combat_any_move_up_down_center" "aim_combat_any_move_up_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

$sequence "combat_unarmed_aim_still_up" {
