//MOVEMENT SEQUENCES

$sequence "move_combat_unarmed_1_up" {
	"combat_unarmed_move_S"
	"combat_unarmed_move_W"
	"combat_unarmed_move_N"
	"combat_unarmed_move_E"
	"combat_unarmed_move_S"
	activity "ACT_RUN_AIM_PISTOL" 1
	blend "move_yaw" -180 180
	addlayer	"combat_unarmed_aim_move_up"
	addlayer	"Combat_Fidget_Layer"
	blendwidth 5
	fadein 0.2
	fadeout 0.2
	node "move_missile"
	fps 30
	loop
	{ event AE_CL_PLAYSOUND 2 "HALO3_FloodCombatBrute.Step" }
	{ event AE_CL_PLAYSOUND 8 "HALO3_FloodCombatBrute.Step" }
