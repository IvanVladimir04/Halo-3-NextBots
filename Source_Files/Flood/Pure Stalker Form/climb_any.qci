//MOVEMENT SEQUENCES


$sequence "move_climb_any_up" {
	"climb_any_move_S"
	"climb_any_move_W"
	"climb_any_move_N"
	"climb_any_move_E"
	"climb_any_move_S"
	activity "ACT_RUN_AIM_PISTOL" 1
	blend "move_yaw" -180 180
	addlayer	"aim_climb_any_move_up"
	blendwidth 5
	fadein 0.2
	fadeout 0.2
	node "move_missile"
	fps 30
	loop
	{ event AE_CL_PLAYSOUND 2 "HALO3_floodpurestalker.Step" }
	{ event AE_CL_PLAYSOUND 8 "HALO3_floodpurestalker.Step" }
	walkframe 145 LX LY
}

