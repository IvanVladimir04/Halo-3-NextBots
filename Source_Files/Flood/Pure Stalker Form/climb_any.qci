	addlayer	"aim_climb_any_still_up"
	fadein 0.2
	fadeout 0.2
	fps 30
	loop
}

//EVADE SEQUENCES

$sequence "evade_right_climb_any_any" {
	"flood_stalker_animations\climb_evade_right.smd"
