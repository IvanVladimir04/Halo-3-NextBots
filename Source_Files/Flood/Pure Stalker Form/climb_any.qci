	fadein 0.2
	fadeout 0.2
	fps 30
	{ event AE_CL_PLAYSOUND 14 "HALO3_floodpurestalker.Step" }
	{ event AE_CL_PLAYSOUND 16 "HALO3_floodpurestalker.Step" }
