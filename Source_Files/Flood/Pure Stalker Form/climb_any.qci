	fps 30
	subtract	"climb_overlay_pose"	0
}

//CLIMB OVERLAY

$sequence "Climb_Fidget_Layer" {
	"climb_overlay"
	fadein 0.2
	fadeout 0.2
	fps 10
	loop
	delta
	hidden
}



//AIMLAYER SEQUENCES

$sequence "aim_climb_any_still_up" {
	"aim_climb_any_still_up_up_right" "aim_climb_any_still_up_up_center"  "aim_climb_any_still_up_up_left"
	"aim_climb_any_still_up_mid_right" "aim_climb_any_still_up_mid_center"  "aim_climb_any_still_up_mid_left"
	"aim_climb_any_still_up_down_right" "aim_climb_any_still_up_down_center" "aim_climb_any_still_up_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}


$sequence "aim_climb_any_move_up" {
	"aim_climb_any_move_up_up_right" "aim_climb_any_move_up_up_center"  "aim_climb_any_move_up_up_left"
	"aim_climb_any_move_up_mid_right" "aim_climb_any_move_up_mid_center"  "aim_climb_any_move_up_mid_left"
	"aim_climb_any_move_up_down_right" "aim_climb_any_move_up_down_center" "aim_climb_any_move_up_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}


//IDLE SEQUENCES

$sequence "climb_any_idle_up_1" {
	"flood_stalker_animations\climb_idle.smd"
