//AIMLAYER SEQUENCES

$sequence "aim_perch_wall_left_pistol_still_up" {
	"aim_perch_wall_left_pistol_still_up_up_right" "aim_perch_wall_left_pistol_still_up_up_center"  "aim_perch_wall_left_pistol_still_up_up_left"
	"aim_perch_wall_left_pistol_still_up_mid_right" "aim_perch_wall_left_pistol_still_up_mid_center"  "aim_perch_wall_left_pistol_still_up_mid_left"
	"aim_perch_wall_left_pistol_still_up_down_right" "aim_perch_wall_left_pistol_still_up_down_center" "aim_perch_wall_left_pistol_still_up_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

$sequence "aim_perch_wall_left_pistol_still_down" {
	"aim_perch_wall_left_pistol_still_down_up_right" "aim_perch_wall_left_pistol_still_down_up_center"  "aim_perch_wall_left_pistol_still_down_up_left"
	"aim_perch_wall_left_pistol_still_down_mid_right" "aim_perch_wall_left_pistol_still_down_mid_center"  "aim_perch_wall_left_pistol_still_down_mid_left"
	"aim_perch_wall_left_pistol_still_down_down_right" "aim_perch_wall_left_pistol_still_down_down_center" "aim_perch_wall_left_pistol_still_down_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}


//IDLE SEQUENCES

$sequence "perch_wall_left_pistol_idle_down" {
	"drone_animations\perch_wall_left_pistol_idle.smd"
	activity "ACT_IDLE_PISTOL" 1
	addlayer	"aim_perch_wall_left_pistol_still_down"
	fadein 0.2
	fadeout 0.2
	fps 30
	loop
}

$sequence "perch_wall_left_pistol_idle_up" {
	"drone_animations\perch_wall_left_pistol_idle.smd"
	activity "ACT_IDLE_PISTOL" 1
	addlayer	"aim_perch_wall_left_pistol_still_up"
	fadein 0.2
	fadeout 0.2
	fps 30
	loop
}

//MOVEMENT-TRANSITORY SEQUENCES
$origin 80 0 30 -90
$sequence "perch_wall_attach_left_start" {
	"drone_animations\perch_wall_left_pistol_enter.smd"
	activity "ACT_ZOMBIE_CLIMB_START" 1
	fadein 0.2
	fadeout 0.2
	fps 30
	{ event 1100 0 "event_halo_3_drones_toggle_wings_on" }
	walkframe 60 LX LY LZ
}

$origin 0 0 0 -90
$sequence "perch_wall_attach_left_end" {
	"drone_animations\perch_wall_left_pistol_exit.smd"
	activity "ACT_ZOMBIE_CLIMB_END" 1
	fadein 0.2
	fadeout 0.2
	fps 30
	{ event 1100 0 "event_halo_3_drones_toggle_wings_on" }
	walkframe 60 LX LY
}

$sequence "perch_wall_left_2_right" {
	"drone_animations\perch_wall_left_pistol_idle_2_perch_wall_right_idle.smd"
	activity "ACT_ZOMBIE_CLIMB_END" 1
	fadein 0.2
	fadeout 0.2
	fps 30
	{ event 1100 0 "event_halo_3_drones_toggle_wings_on" }
	walkframe 60 LXR LYR LZR LZR
}

//ORDER SEQUENCES



$sequence "point_perch_wall_left_pistol" {
	"drone_animations\perch_wall_left_pistol_point.smd"
	activity "ACT_GMOD_GESTURE_POINT" 1
	fadein 0.2
	fadeout 0.2
	fps 30
}

$sequence "shakefist_perch_wall_left_pistol" {
	"drone_animations\perch_wall_left_pistol_shakefist.smd"
	activity "ACT_GMOD_GESTURE_BECON" 1
	fadein 0.2
	fadeout 0.2
	fps 30
	{ event 1100 7 "event_halo_3_drones_toggle_wings_on" }
	{ event 1100 8 "event_halo_3_drones_toggle_wings_off" }
}



