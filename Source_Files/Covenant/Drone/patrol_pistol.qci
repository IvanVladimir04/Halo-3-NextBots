//IDLE SEQUENCES

$sequence "patrol_pistol_idle_up" {
	"drone_animations\patrol_pistol_idle.smd"
	activity "ACT_IDLE_PISTOL" 1
	addlayer	"aim_patrol_unarmed_still_up"
	fadein 0.2
	fadeout 0.2
	fps 30
	loop
}

$sequence "patrol_pistol_idle_down" {
	"drone_animations\patrol_pistol_idle.smd"
	activity "ACT_IDLE_PISTOL" 1
	addlayer	"aim_patrol_unarmed_still_down"
