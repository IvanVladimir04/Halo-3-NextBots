//MOVEMENT SEQUENCES

$sequence "move_patrol_pistol_up" {
	"patrol_pistol_move_S"
	"patrol_pistol_move_W"
	"patrol_pistol_move_N"
	"patrol_pistol_move_E"
	"patrol_pistol_move_S"
	activity "ACT_RUN_AIM_PISTOL" 1
	blend "move_yaw" -180 180
	addlayer	"aim_patrol_pistol_move_up"
	blendwidth 5
	fadein 0.2
	fadeout 0.2
	node "move_pistol"
	fps 30
	loop
	{ event AE_CL_PLAYSOUND 2 "HALO3_Drone.Step" }
	{ event AE_CL_PLAYSOUND 8 "HALO3_Drone.Step" }
	walkframe 145 LX LY
}

$sequence "move_patrol_pistol_down" {
