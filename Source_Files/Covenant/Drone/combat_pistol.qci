	{ event 1100 7 "event_halo_3_drones_toggle_wings_on" }
	{ event 1100 147 "event_halo_3_drones_toggle_wings_off" }
}


//MOVEMENT SEQUENCES

$sequence "move_combat_pistol_1_up" {
	"combat_pistol_move_S"
	"combat_pistol_move_W"
	"combat_pistol_move_1_N"
	"combat_pistol_move_E"
	"combat_pistol_move_S"
