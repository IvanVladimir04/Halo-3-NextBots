//AIMLAYER SEQUENCES

$sequence "aim_combat_pistol_still_up" {
	"aim_combat_pistol_still_up_up_right" "aim_combat_pistol_still_up_up_center"  "aim_combat_pistol_still_up_up_left"
	"aim_combat_pistol_still_up_mid_right" "aim_combat_pistol_still_up_mid_center"  "aim_combat_pistol_still_up_mid_left"
	"aim_combat_pistol_still_up_down_right" "aim_combat_pistol_still_up_down_center" "aim_combat_pistol_still_up_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

$sequence "aim_combat_pistol_still_down" {
	"aim_combat_pistol_still_down_up_right" "aim_combat_pistol_still_down_up_center"  "aim_combat_pistol_still_down_up_left"
	"aim_combat_pistol_still_down_mid_right" "aim_combat_pistol_still_down_mid_center"  "aim_combat_pistol_still_down_mid_left"
	"aim_combat_pistol_still_down_down_right" "aim_combat_pistol_still_down_down_center" "aim_combat_pistol_still_down_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

$sequence "aim_combat_pistol_move_up" {
	"aim_combat_pistol_move_up_up_right" "aim_combat_pistol_move_up_up_center"  "aim_combat_pistol_move_up_up_left"
	"aim_combat_pistol_move_up_mid_right" "aim_combat_pistol_move_up_mid_center"  "aim_combat_pistol_move_up_mid_left"
	"aim_combat_pistol_move_up_down_right" "aim_combat_pistol_move_up_down_center" "aim_combat_pistol_move_up_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

$sequence "aim_combat_pistol_move_down" {
	"aim_combat_pistol_move_down_up_right" "aim_combat_pistol_move_down_up_center"  "aim_combat_pistol_move_down_up_left"
	"aim_combat_pistol_move_down_mid_right" "aim_combat_pistol_move_down_mid_center"  "aim_combat_pistol_move_down_mid_left"
	"aim_combat_pistol_move_down_down_right" "aim_combat_pistol_move_down_down_center" "aim_combat_pistol_move_down_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

//IDLE SEQUENCES

$sequence "combat_pistol_idle_down" {
	"drone_animations\combat_pistol_idle.smd"
	activity "ACT_IDLE_PISTOL" 1
	addlayer	"aim_combat_pistol_still_down"
	fadein 0.2
	fadeout 0.2
	fps 30
	loop
}

$sequence "combat_pistol_idle_up" {
	"drone_animations\combat_pistol_idle.smd"
	activity "ACT_IDLE_PISTOL" 1
	addlayer	"aim_combat_pistol_still_up"
	fadein 0.2
	fadeout 0.2
	fps 30
	loop
}

$sequence "combat_pistol_idle_posing_1" {
	"drone_animations\combat_pistol_posing_var1.smd"
	activity "ACT_IDLE_PISTOL" 1
	addlayer	"aim_combat_pistol_still_down"
	fadein 0.2
	fadeout 0.2
	fps 30
	loop
}

$sequence "combat_pistol_idle_posing_2" {
	"drone_animations\combat_pistol_posing_var2.smd"
	activity "ACT_IDLE_PISTOL" 1
	addlayer	"aim_combat_pistol_still_down"
	fadein 0.2
	fadeout 0.2
	fps 30
