//READY AND PUT AWAY ANIMATIONS

$animation "combat_ball_put_away_pose" "elite_animations\combat_ball_put_away.smd" {
	fps 30
	loop
	frame 4 4
}

$animation "combat_ball_put_away" "elite_animations\combat_ball_put_away.smd" {
	fps 30
	subtract	"combat_ball_put_away_pose"	0
}

$animation "combat_ball_ready_pose" "elite_animations\combat_ball_ready.smd" {
	fps 30
	loop
	frame 25 25
}

$animation "combat_ball_ready" "elite_animations\combat_ball_ready.smd" {
	fps 30
	subtract	"combat_ball_ready_pose"	0
}

//AIMLAYER SEQUENCES

$sequence "aim_combat_ball_still_up" {
	"aim_combat_ball_still_up_up_right" "aim_combat_ball_still_up_up_center"  "aim_combat_ball_still_up_up_left"
	"aim_combat_ball_still_up_mid_right" "aim_combat_ball_still_up_mid_center"  "aim_combat_ball_still_up_mid_left"
	"aim_combat_ball_still_up_down_right" "aim_combat_ball_still_up_down_center" "aim_combat_ball_still_up_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

$sequence "aim_combat_ball_still_down" {
	"aim_combat_ball_still_down_up_right" "aim_combat_ball_still_down_up_center"  "aim_combat_ball_still_down_up_left"
	"aim_combat_ball_still_down_mid_right" "aim_combat_ball_still_down_mid_center"  "aim_combat_ball_still_down_mid_left"
	"aim_combat_ball_still_down_down_right" "aim_combat_ball_still_down_down_center" "aim_combat_ball_still_down_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

$sequence "aim_combat_ball_move_up" {
	"aim_combat_ball_move_up_up_right" "aim_combat_ball_move_up_up_center"  "aim_combat_ball_move_up_up_left"
	"aim_combat_ball_move_up_mid_right" "aim_combat_ball_move_up_mid_center"  "aim_combat_ball_move_up_mid_left"
	"aim_combat_ball_move_up_down_right" "aim_combat_ball_move_up_down_center" "aim_combat_ball_move_up_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

$sequence "aim_combat_ball_move_down" {
	"aim_combat_ball_move_down_up_right" "aim_combat_ball_move_down_up_center"  "aim_combat_ball_move_down_up_left"
	"aim_combat_ball_move_down_mid_right" "aim_combat_ball_move_down_mid_center"  "aim_combat_ball_move_down_mid_left"
	"aim_combat_ball_move_down_down_right" "aim_combat_ball_move_down_down_center" "aim_combat_ball_move_down_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

//IDLE SEQUENCES

$sequence "combat_ball_idle_down" {
	"elite_animations\combat_ball_idle.smd"
	activity "ACT_IDLE_PACKAGE_RELAXED" 1
	addlayer	"aim_combat_ball_still_down"
	fadein 0.2
	fadeout 0.2
	fps 30
	loop
}

$sequence "combat_ball_idle_up" {
	"elite_animations\combat_ball_idle.smd"
	activity "ACT_IDLE_PACKAGE" 1
	addlayer	"aim_combat_ball_still_up"
	fadein 0.2
	fadeout 0.2
	fps 30
	loop
}


//MOVEMENT SEQUENCES

$sequence "move_combat_ball_up" {
	"combat_ball_move_S"
	"combat_ball_move_W"
	"combat_ball_move_N"
	"combat_ball_move_E"
	"combat_ball_move_S"
	activity "ACT_RUN_PACKAGE" 1
	blend "move_yaw" -180 180
	addlayer	"aim_combat_ball_move_up"
	blendwidth 5
	fadein 0.2
	fadeout 0.2
	node "move_ball"
	fps 30
	loop
	{ event AE_CL_PLAYSOUND 2 "HALO3_Elite.Step" }
	{ event AE_CL_PLAYSOUND 8 "HALO3_Elite.Step" }
	walkframe 145 LX LY
}

