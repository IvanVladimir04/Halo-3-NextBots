//AIMLAYER SEQUENCES

$sequence "aim_combat_rifle_still_up" {
	"aim_combat_rifle_still_up_up_right" "aim_combat_rifle_still_up_up_center"  "aim_combat_rifle_still_up_up_left"
	"aim_combat_rifle_still_up_mid_right" "aim_combat_rifle_still_up_mid_center"  "aim_combat_rifle_still_up_mid_left"
	"aim_combat_rifle_still_up_down_right" "aim_combat_rifle_still_up_down_center" "aim_combat_rifle_still_up_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

$sequence "aim_combat_rifle_still_down" {
	"aim_combat_rifle_still_down_up_right" "aim_combat_rifle_still_down_up_center"  "aim_combat_rifle_still_down_up_left"
	"aim_combat_rifle_still_down_mid_right" "aim_combat_rifle_still_down_mid_center"  "aim_combat_rifle_still_down_mid_left"
	"aim_combat_rifle_still_down_down_right" "aim_combat_rifle_still_down_down_center" "aim_combat_rifle_still_down_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}


$sequence "aim_combat_rifle_move_up" {
	"aim_combat_rifle_move_up_up_right" "aim_combat_rifle_move_up_up_center"  "aim_combat_rifle_move_up_up_left"
	"aim_combat_rifle_move_up_mid_right" "aim_combat_rifle_move_up_mid_center"  "aim_combat_rifle_move_up_mid_left"
	"aim_combat_rifle_move_up_down_right" "aim_combat_rifle_move_up_down_center" "aim_combat_rifle_move_up_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

$sequence "aim_combat_rifle_move_down" {
	"aim_combat_rifle_move_down_up_right" "aim_combat_rifle_move_down_up_center"  "aim_combat_rifle_move_down_up_left"
	"aim_combat_rifle_move_down_mid_right" "aim_combat_rifle_move_down_mid_center"  "aim_combat_rifle_move_down_mid_left"
	"aim_combat_rifle_move_down_down_right" "aim_combat_rifle_move_down_down_center" "aim_combat_rifle_move_down_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

