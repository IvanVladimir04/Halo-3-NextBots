//AIMLAYER SEQUENCES

$sequence "aim_combat_unarmed_still_up" {
	"aim_combat_unarmed_still_up_up_right" "aim_combat_unarmed_still_up_up_center"  "aim_combat_unarmed_still_up_up_left"
	"aim_combat_unarmed_still_up_mid_right" "aim_combat_unarmed_still_up_mid_center"  "aim_combat_unarmed_still_up_mid_left"
	"aim_combat_unarmed_still_up_down_right" "aim_combat_unarmed_still_up_down_center" "aim_combat_unarmed_still_up_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

$sequence "aim_combat_unarmed_still_down" {
	"aim_combat_unarmed_still_down_up_right" "aim_combat_unarmed_still_down_up_center"  "aim_combat_unarmed_still_down_up_left"
	"aim_combat_unarmed_still_down_mid_right" "aim_combat_unarmed_still_down_mid_center"  "aim_combat_unarmed_still_down_mid_left"
	"aim_combat_unarmed_still_down_down_right" "aim_combat_unarmed_still_down_down_center" "aim_combat_unarmed_still_down_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

$sequence "aim_combat_unarmed_move_up" {
	"aim_combat_unarmed_move_up_up_right" "aim_combat_unarmed_move_up_up_center"  "aim_combat_unarmed_move_up_up_left"
	"aim_combat_unarmed_move_up_mid_right" "aim_combat_unarmed_move_up_mid_center"  "aim_combat_unarmed_move_up_mid_left"
	"aim_combat_unarmed_move_up_down_right" "aim_combat_unarmed_move_up_down_center" "aim_combat_unarmed_move_up_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

$sequence "aim_combat_unarmed_move_down" {
	"aim_combat_unarmed_move_down_up_right" "aim_combat_unarmed_move_down_up_center"  "aim_combat_unarmed_move_down_up_left"
	"aim_combat_unarmed_move_down_mid_right" "aim_combat_unarmed_move_down_mid_center"  "aim_combat_unarmed_move_down_mid_left"
	"aim_combat_unarmed_move_down_down_right" "aim_combat_unarmed_move_down_down_center" "aim_combat_unarmed_move_down_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
	fadein 0.3
	fadeout 0.3
	hidden
}

//IDLE SEQUENCES

$sequence "combat_unarmed_idle_down" {
	"elite_animations\combat_unarmed_idle_var0.smd"
	activity "ACT_IDLE_RPG_RELAXED" 1
	addlayer	"aim_combat_unarmed_still_down"
	fadein 0.2
	fadeout 0.2
	fps 30
	loop
}

$sequence "combat_unarmed_idle_up" {
	"elite_animations\combat_unarmed_idle_var1.smd"
	activity "ACT_IDLE_RPG" 1
	addlayer	"aim_combat_unarmed_still_up"
	fadein 0.2
	fadeout 0.2
	fps 30
	loop
}


//MOVEMENT SEQUENCES

$sequence "move_combat_unarmed_up" {
	"combat_unarmed_move_S"
	"combat_unarmed_move_W"
	"combat_unarmed_move_N"
	"combat_unarmed_move_E"
	"combat_unarmed_move_S"
	activity "ACT_RUN_RPG" 1
	blend "move_yaw" -180 180
	addlayer	"aim_combat_unarmed_move_up"
	blendwidth 5
	fadein 0.2
	fadeout 0.2
	node "move_unarmed"
	fps 30
	loop
	{ event AE_CL_PLAYSOUND 2 "HALO3_Elite.Step" }
	{ event AE_CL_PLAYSOUND 8 "HALO3_Elite.Step" }
	walkframe 145 LX LY
}

