//MOVEMENT SEQUENCES

$sequence "move_combat_rifle_up" {
	"combat_rifle_move_S"
	"combat_rifle_move_W"
	"combat_rifle_move_N"
	"combat_rifle_move_E"
	"combat_rifle_move_S"
	activity "ACT_RUN_AIM_PISTOL" 1
	blend "move_yaw" -180 180
	addlayer	"aim_combat_rifle_move_up"
	blendwidth 5
	fadein 0.2
	fadeout 0.2
	node "move_missile"
	fps 30
	loop
	{ event AE_CL_PLAYSOUND 2 "HALO3_Brute.Step" }
	{ event AE_CL_PLAYSOUND 8 "HALO3_Brute.Step" }
	walkframe 145 LX LY
}

$sequence "move_combat_rifle_down" {
	"combat_rifle_move_S"
	"combat_rifle_move_W"
	"combat_rifle_move_N"
	"combat_rifle_move_E"
	"combat_rifle_move_S"
	activity "ACT_RUN_PISTOL" 1
	blend "move_yaw" -180 180
	addlayer	"aim_combat_rifle_move_down"
	blendwidth 5
	fadein 0.2
	fadeout 0.2
	node "move_missile"
	fps 30
	loop
	{ event AE_CL_PLAYSOUND 2 "HALO3_Brute.Step" }
	{ event AE_CL_PLAYSOUND 8 "HALO3_Brute.Step" }
	walkframe 145 LX LY
}

$sequence "walk_combat_rifle_up" {
