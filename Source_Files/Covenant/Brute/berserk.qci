	addlayer	"aim_berserk_unarmed_still_down"
	fadein 0.2
	fadeout 0.2
	fps 30
	loop
}


//MOVEMENT SEQUENCES

$sequence "move_berserk_unarmed_down" {
	"berserk_unarmed_move_S"
	"berserk_unarmed_move_W"
	"berserk_unarmed_move_N"
	"berserk_unarmed_move_E"
	"berserk_unarmed_move_S"
	activity "ACT_RUN_PISTOL" 1
	blend "move_yaw" -180 180
	addlayer	"aim_berserk_unarmed_move_down"
	blendwidth 5
	fadein 0.2
	fadeout 0.2
	node "move_missile"
	fps 30
	loop
	{ event AE_CL_PLAYSOUND 2 "HALO3_Brute.Step" }
	{ event AE_CL_PLAYSOUND 8 "HALO3_Brute.Step" }
	walkframe 145 LX LY
}

