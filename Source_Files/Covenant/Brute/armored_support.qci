}

$sequence "aim_armored_support_move_down" {
	"aim_armored_support_move_down_up_right" "aim_armored_support_move_down_up_center"  "aim_armored_support_move_down_up_left"
	"aim_armored_support_move_down_mid_right" "aim_armored_support_move_down_mid_center"  "aim_armored_support_move_down_mid_left"
	"aim_armored_support_move_down_down_right" "aim_armored_support_move_down_down_center" "aim_armored_support_move_down_down_left"
