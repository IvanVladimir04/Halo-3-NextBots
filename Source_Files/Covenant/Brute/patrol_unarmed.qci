//MOVEMENT SEQUENCES

$sequence "move_patrol_unarmed_up" {
	"patrol_unarmed_move_S"
	"patrol_unarmed_move_W"
	"patrol_unarmed_move_N"
	"patrol_unarmed_move_E"
	"patrol_unarmed_move_S"
	activity "ACT_RUN_RELAXED_PISTOL" 1
	blend "move_yaw" -180 180
	addlayer	"aim_patrol_unarmed_move_down"
	blendwidth 5
	fadein 0.2
	fadeout 0.2
	node "move_missile"
	fps 30
	loop
	{ event AE_CL_PLAYSOUND 2 "HALO3_Brute.Step" }
	{ event AE_CL_PLAYSOUND 8 "HALO3_Brute.Step" }
	walkframe 145 LX LY
}


