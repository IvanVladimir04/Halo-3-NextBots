	fps 30
	loop
}


//MOVEMENT SEQUENCES

$sequence "move_combat_pistol_up" {
	"combat_pistol_move_S"
	"combat_pistol_move_W"
	"combat_pistol_move_N"
	"combat_pistol_move_E"
	"combat_pistol_move_S"
	activity "ACT_RUN_AIM_PISTOL" 1
	blend "move_yaw" -180 180
	addlayer	"aim_combat_pistol_move_up"
	blendwidth 5
	fadein 0.2
	fadeout 0.2
	node "move_missile"
	fps 30
	loop
	{ event AE_CL_PLAYSOUND 2 "HALO3_Brute.Step" }
	{ event AE_CL_PLAYSOUND 8 "HALO3_Brute.Step" }
	walkframe 145 LX LY
}

$sequence "move_combat_pistol_down" {
	"combat_pistol_move_S"
	"combat_pistol_move_W"
	"combat_pistol_move_N"
	"combat_pistol_move_E"
	"combat_pistol_move_S"
	activity "ACT_RUN_PISTOL" 1
	blend "move_yaw" -180 180
