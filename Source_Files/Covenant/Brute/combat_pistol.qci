}

//IDLE SEQUENCES

$sequence "combat_pistol_idle_down" {
	"brute_animations\combat_pistol_idle_var1.smd"
	activity "ACT_IDLE_PISTOL" 1
	addlayer	"aim_combat_pistol_still_down"
	fadein 0.2
	fadeout 0.2
	fps 30
	loop
}

$sequence "combat_pistol_idle_up" {
	"brute_animations\combat_pistol_idle_var1.smd"
	activity "ACT_IDLE_PISTOL" 1
	addlayer	"aim_combat_pistol_still_up"
	fadein 0.2
