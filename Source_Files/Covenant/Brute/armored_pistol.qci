	"aim_armored_pistol_still_up_mid_right" "aim_armored_pistol_still_up_mid_center"  "aim_armored_pistol_still_up_mid_left"
	"aim_armored_pistol_still_up_down_right" "aim_armored_pistol_still_up_down_center" "aim_armored_pistol_still_up_down_left"
	blend "aim_yaw" -45 45
	blend "aim_pitch" -90 90
	blendwidth 3
	delta
