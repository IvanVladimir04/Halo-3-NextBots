//MOVEMENT SEQUENCES

$sequence "move_crouch_rifle_up" {
	"crouch_rifle_move_S"
	"crouch_rifle_move_W"
	"crouch_rifle_move_N"
	"crouch_rifle_move_E"
	"crouch_rifle_move_S"
	activity "ACT_RUN_AIM_PISTOL" 1
	blend "move_yaw" -180 180
	addlayer	"aim_crouch_rifle_move_up"
	blendwidth 5
	fadein 0.2
	fadeout 0.2
	node "move_missile"
	fps 30
	loop
	{ event AE_CL_PLAYSOUND 2 "HALO3_Jackal.Step" }
	{ event AE_CL_PLAYSOUND 8 "HALO3_Jackal.Step" }
	walkframe 145 LX LY
}

