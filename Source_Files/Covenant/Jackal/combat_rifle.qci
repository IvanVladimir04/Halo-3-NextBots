	"combat_rifle_idle_2_combat_walk_W"
	"combat_rifle_idle_2_combat_walk_N"
	"combat_rifle_idle_2_combat_walk_E"
	"combat_rifle_idle_2_combat_walk_S"
	activity "ACT_READINESS_RELAXED_TO_STIMULATED" 1
	addlayer	"aim_combat_rifle_move_down"
	blend "move_yaw" -180 180
	blendwidth 5
	fadein 0.2
	fadeout 0.2
	node "walk_pistol"
	fps 30
	loop
	walkframe 145 LX LY
}

