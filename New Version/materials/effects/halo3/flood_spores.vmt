"UnlitGeneric"
{
	"$basetexture"	"effects/halo3/flood_spores"
	"$translucent"	1
	"$vertexcolor"	1
	"$vertexalpha"	1

"Proxies"
   {
      "AnimatedTexture"
      {
         "animatedtexturevar" "$basetexture"
         "animatedtextureframenumvar" "$frame"
         "animatedtextureframerate" "10"
      }
   }


}