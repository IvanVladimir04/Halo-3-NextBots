"UnlitGeneric"
{
	"$additive" 1
	"$translucent" 1
	"$vertexcolor" 1
	"$vertexalpha" 1
	"$decal" 1

	"$alpha" 1	

	"$basetexture" "models\halo_3\shared\overshield"


        "Proxies"
        {
                "AnimatedTexture"
                {
                         "animatedtexturevar" "$basetexture"
                         "animatedtextureframenumvar" "$frame"
                         "animatedtextureframerate" 30.00
                }

        }
}
