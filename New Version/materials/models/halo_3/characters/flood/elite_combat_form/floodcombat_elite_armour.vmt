"VertexlitGeneric"
{
	"$basetexture" "models/halo_3/characters/flood/elite_combat_form/floodcombat_elite_diffuse"
	"$bumpmap" "models/halo_3/characters/flood/elite_combat_form/floodcombat_elite_zbump"
	"$blendtintbybasealpha" "1"
	"$phong" 1
	"$phongboost" "3"
	"$phongexponent" "6"
	"$phongfresnelranges" "[.5 .85 6]"
	"$model" 1
	"$phongalbedotint" "1"
	$ambientocclusion 1
	"$rimlight"	"1"
	"$rimlightexponent"	"15"
	"$rimlightboost"	"5"
	"$envmap"	"env_cubemap"
	"$lightwarptexture"			"models/halo_3/shared/lw_metaldull"
	"$nodecal"				1

	Proxies
	{
		Clamp
		{
			min 5
			max 5
			srcVar1 "$color2"
			resultVar "$emissiveBlendTint"
		}
		Clamp
		{
			min 0
			max 1
			srcVar1 "$color"
			resultVar "$color"
		}
		Clamp
		{
			min 0
			max 1
			srcVar1 "$color"
			resultVar "$phongtint"
		}
		Clamp
		{
			min 0
			max 0.05
			srcVar1 "$color"
			resultVar "$envmaptint"
		}
	}
}
