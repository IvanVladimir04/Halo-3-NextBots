"VertexLitGeneric"
{



	"$basetexture" "models/halo_3/characters/alliance/elite/mp_raptor"
	"$bumpmap" "models/halo_3/characters/alliance/elite/mp_raptor_zbump"
	"$normalmapalphaenvmapmask" 1
	"$nodecal" 1

	"$envmap" "models/halo_3/shared/cyborg_reflection_armor.hdr"
	"$envmaptint" "[3.125 .85 0.15]"

	"$phong" 1
	"$phongexponent" 5
	"$phongboost" 1.25
	"$phongfresnelranges" "[1 1 1]"
