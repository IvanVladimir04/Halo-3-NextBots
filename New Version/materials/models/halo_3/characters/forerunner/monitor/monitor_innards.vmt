	Proxies
	{
		Clamp
		{
			min 0
			max 1
			srcVar1 "$color2"
			resultVar "$emissiveBlendTint"
		}
		Clamp
		{
			min 0
			max 1
			srcVar1 "$color"
			resultVar "$color"
		}
		Clamp
		{
			min 0.8
			max 1
			srcVar1 "$color"
			resultVar "$phongtint"
		}
		Clamp
		{
			min 0.2
			max 0.25
			srcVar1 "$color2"
			resultVar "$envmaptint"
		}
	}


}

